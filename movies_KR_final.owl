<?xml version="1.0"?>
<rdf:RDF xmlns="http://example.org/movies-ontology#"
     xml:base="http://example.org/movies-ontology"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
    <owl:Ontology rdf:about="http://example.org/movies-ontology"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://example.org/movies-ontology#actsIn -->

    <owl:ObjectProperty rdf:about="http://example.org/movies-ontology#actsIn">
        <owl:inverseOf rdf:resource="http://example.org/movies-ontology#hasActor"/>
        <rdfs:domain rdf:resource="http://example.org/movies-ontology#Actor"/>
        <rdfs:range rdf:resource="http://example.org/movies-ontology#Movie"/>
    </owl:ObjectProperty>
    


    <!-- http://example.org/movies-ontology#availableOn -->

    <owl:ObjectProperty rdf:about="http://example.org/movies-ontology#availableOn">
        <rdfs:domain rdf:resource="http://example.org/movies-ontology#Movie"/>
        <rdfs:range rdf:resource="http://example.org/movies-ontology#StreamingPlatform"/>
    </owl:ObjectProperty>
    


    <!-- http://example.org/movies-ontology#directs -->

    <owl:ObjectProperty rdf:about="http://example.org/movies-ontology#directs">
        <owl:inverseOf rdf:resource="http://example.org/movies-ontology#hasDirector"/>
        <rdfs:domain rdf:resource="http://example.org/movies-ontology#Director"/>
        <rdfs:range rdf:resource="http://example.org/movies-ontology#Movie"/>
    </owl:ObjectProperty>
    


    <!-- http://example.org/movies-ontology#hasActor -->

    <owl:ObjectProperty rdf:about="http://example.org/movies-ontology#hasActor">
        <rdfs:domain rdf:resource="http://example.org/movies-ontology#Movie"/>
        <rdfs:range rdf:resource="http://example.org/movies-ontology#Actor"/>
    </owl:ObjectProperty>
    


    <!-- http://example.org/movies-ontology#hasDirector -->

    <owl:ObjectProperty rdf:about="http://example.org/movies-ontology#hasDirector">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://example.org/movies-ontology#Movie"/>
        <rdfs:range rdf:resource="http://example.org/movies-ontology#Director"/>
    </owl:ObjectProperty>
    


    <!-- http://example.org/movies-ontology#hasGenre -->

    <owl:ObjectProperty rdf:about="http://example.org/movies-ontology#hasGenre">
        <rdfs:domain rdf:resource="http://example.org/movies-ontology#Movie"/>
        <rdfs:range rdf:resource="http://example.org/movies-ontology#Genre"/>
    </owl:ObjectProperty>
    


    <!-- http://example.org/movies-ontology#hasSequel -->

    <owl:ObjectProperty rdf:about="http://example.org/movies-ontology#hasSequel">
        <rdfs:domain rdf:resource="http://example.org/movies-ontology#Movie"/>
        <rdfs:range rdf:resource="http://example.org/movies-ontology#Movie"/>
        <owl:propertyDisjointWith rdf:resource="http://example.org/movies-ontology#partOfFranchise"/>
    </owl:ObjectProperty>
    


    <!-- http://example.org/movies-ontology#leadActorOf -->

    <owl:ObjectProperty rdf:about="http://example.org/movies-ontology#leadActorOf">
        <rdfs:subPropertyOf rdf:resource="http://example.org/movies-ontology#actsIn"/>
        <rdfs:domain rdf:resource="http://example.org/movies-ontology#LeadActor"/>
        <rdfs:range rdf:resource="http://example.org/movies-ontology#Movie"/>
    </owl:ObjectProperty>
    


    <!-- http://example.org/movies-ontology#partOfFranchise -->

    <owl:ObjectProperty rdf:about="http://example.org/movies-ontology#partOfFranchise">
        <rdfs:domain rdf:resource="http://example.org/movies-ontology#Movie"/>
        <rdfs:range rdf:resource="http://example.org/movies-ontology#Franchise"/>
    </owl:ObjectProperty>
    


    <!-- http://example.org/movies-ontology#wonAward -->

    <owl:ObjectProperty rdf:about="http://example.org/movies-ontology#wonAward"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://example.org/movies-ontology#hasRating -->

    <owl:DatatypeProperty rdf:about="http://example.org/movies-ontology#hasRating">
        <rdfs:domain rdf:resource="http://example.org/movies-ontology#Movie"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
    </owl:DatatypeProperty>
    


    <!-- http://example.org/movies-ontology#hasReleaseYear -->

    <owl:DatatypeProperty rdf:about="http://example.org/movies-ontology#hasReleaseYear">
        <rdfs:domain rdf:resource="http://example.org/movies-ontology#Movie"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://example.org/movies-ontology#Actor -->

    <owl:Class rdf:about="http://example.org/movies-ontology#Actor">
        <rdfs:subClassOf rdf:resource="http://example.org/movies-ontology#Person"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://example.org/movies-ontology#actsIn"/>
                <owl:allValuesFrom rdf:resource="http://example.org/movies-ontology#Movie"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="http://example.org/movies-ontology#Director"/>
    </owl:Class>
    


    <!-- http://example.org/movies-ontology#Award -->

    <owl:Class rdf:about="http://example.org/movies-ontology#Award"/>
    


    <!-- http://example.org/movies-ontology#AwardWinningMovie -->

    <owl:Class rdf:about="http://example.org/movies-ontology#AwardWinningMovie">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://example.org/movies-ontology#Movie"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://example.org/movies-ontology#wonAward"/>
                        <owl:someValuesFrom rdf:resource="http://example.org/movies-ontology#Award"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://example.org/movies-ontology#Movie"/>
    </owl:Class>
    


    <!-- http://example.org/movies-ontology#BlockbusterMovie -->

    <owl:Class rdf:about="http://example.org/movies-ontology#BlockbusterMovie">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://example.org/movies-ontology#Movie"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://example.org/movies-ontology#wonAward"/>
                        <owl:someValuesFrom rdf:resource="http://example.org/movies-ontology#Award"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://example.org/movies-ontology#hasRating"/>
                        <owl:someValuesFrom>
                            <rdfs:Datatype>
                                <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
                                <owl:withRestrictions rdf:parseType="Collection">
                                    <rdf:Description>
                                        <xsd:minInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">8</xsd:minInclusive>
                                    </rdf:Description>
                                </owl:withRestrictions>
                            </rdfs:Datatype>
                        </owl:someValuesFrom>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://example.org/movies-ontology#Movie"/>
    </owl:Class>
    


    <!-- http://example.org/movies-ontology#Comedy -->

    <owl:Class rdf:about="http://example.org/movies-ontology#Comedy">
        <rdfs:subClassOf rdf:resource="http://example.org/movies-ontology#Genre"/>
    </owl:Class>
    


    <!-- http://example.org/movies-ontology#ComedyMovie -->

    <owl:Class rdf:about="http://example.org/movies-ontology#ComedyMovie">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://example.org/movies-ontology#Movie"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://example.org/movies-ontology#hasGenre"/>
                        <owl:hasValue rdf:resource="http://example.org/movies-ontology#Comedy"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    


    <!-- http://example.org/movies-ontology#Director -->

    <owl:Class rdf:about="http://example.org/movies-ontology#Director">
        <rdfs:subClassOf rdf:resource="http://example.org/movies-ontology#Person"/>
    </owl:Class>
    


    <!-- http://example.org/movies-ontology#Drama -->

    <owl:Class rdf:about="http://example.org/movies-ontology#Drama">
        <rdfs:subClassOf rdf:resource="http://example.org/movies-ontology#Genre"/>
        <rdfs:subClassOf>
            <owl:Class>
                <owl:complementOf rdf:resource="http://example.org/movies-ontology#Comedy"/>
            </owl:Class>
        </rdfs:subClassOf>
    </owl:Class>
    


